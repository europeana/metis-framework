{{> atoms/iconography/svgicons-search }}
{{> atoms/iconography/svgicons-user }}

{{> organisms/global/header_v2}}



<main role="main" class="l-page">
<div class="lc">

  <div class="l-content">

    <div class="page_header">
      {{> molecules/navigation/breadcrumbs}}
    </div>

    <div class="maincontent">
    	<div class="textcontent">

			<div class="articleheader">

				<h1 class="title">{{ headline.medium }}</h1>

				<div class="item-meta-primary">
					{{> molecules-item-author }}
					{{> molecules-item-readtime }}
				</div>

				<div class="item-meta-secondary">
					{{> molecules-item-date }}
					{{> molecules-item-tags }}
					{{> molecules-item-comments }}
				</div>

			</div>

			{{! Article Body }}
			<div class="prose">
			{{> organisms-article-body-text }}
			</div>
		</div>
		{{! END textcontent }}


		{{> molecules-related-noimages(title: "You might also like:") }}


    </div>
    {{! END maincontent }}
  </div>
  {{! END l-content }}


  <div class="l-sidebar js-getstickywidth">

  {{> molecules-help-contact(title: "About the Author") }}


  </div>
  {{! end sidebar }}

</div>
</main>
<!-- End Main -->


{{> organisms/global/footer_subsite }}

