{{> atoms/iconography/svgicons-pro}}

{{> organisms/global/header}}



<!-- Start content -->
<main role="main" class="l-page">
<div class="lc">
	<!-- Start grid -->
	<div class="grid-main">

		<div class="l-content">

		<div class="l-breadcrumbs">
			{{> molecules/navigation/breadcrumbs}}
		</div>
		

		<div id="maincontent" class="maincontent">

			<div class="l-twoup">

			 	<div class="l-photo">
					<img src="http://placeimg.com/300/400/tech" alt="photo of Publication"/>
				</div><!--

			 --><div class="l-detail textcontent">

			 		<h1 class="title">Press Release Title</h1>

					<div class="item-meta-secondary">
						{{> molecules-item-date }}
						{{> molecules-item-tags }}
					</div>

					<div class="prose">
					{{> organisms-article-short }}
					</div>

				</div>
			</div>


		</div>
		</div><!--

	 --><div class="l-sidebar">
			<div class="inner">
				{{> molecules-help-contact(title: "Need Help?") }}
			</div>
		</div>
	</div>
	<!-- End grid -->
</div>
</main>
<!-- End Content -->


{{> organisms/global/footer }}

